            <ab  facs="#facs_215_r2">
               <lb facs="#facs_215_r2l1" n="N002"/><seg type="name">Nahmen und Class</seg>
               <lb facs="#facs_215_r2l2" n="N003"/><rs type="person" ref="wkfm__43958">Pohamb Johann</rs> <choice><expan/><abbr>burg:</abbr></choice> Handlsmann
               <lb facs="#facs_215_r2l3" n="N004"/>Firma ad Raggion
               <lb facs="#facs_215_r2l4" n="N005"/><rs type="person" ref="wkfm__47374">Johann Pohamb</rs> laut Erklährung sine dato N<hi style="superscript">o</hi> 132 und weitherer
               <lb facs="#facs_215_r2l5" n="N006"/>Erklährung dd<hi style="superscript">to</hi> 16<hi style="superscript">ten</hi> <choice><expan/><abbr>Xbr:</abbr></choice> 743 N<hi style="superscript">o</hi> 138
               <lb facs="#facs_215_r2l6" n="N007"/><seg type="owner">Procura und Firmae Trager</seg>
               <lb facs="#facs_215_r2l7" n="N008"/><seg type="oblatorien">Oblatorien und Avocatorien</seg>
               <lb facs="#facs_215_r2l8" n="N009"/><seg type="fundi">Fundi Ausweisung</seg>
               <lb facs="#facs_215_r2l9" n="N010"/>Der <rs type="person" ref="wkfm__43958">Johann Pohamb</rs> zu Ausweisung seines Funi eine ungefertigte
               <lb facs="#facs_215_r2l10" n="N011"/>Specification sine dato N<hi style="superscript">o</hi> 1206 eingeleget daß er besize <hi>an</hi>
               <lb facs="#facs_215_r2l11" n="N012"/>an Waren 15106 fl 27 x<hi style="superscript">r</hi>
               <lb facs="#facs_215_r2l12" n="N013"/>an debitoren 18239 fl 5 x<hi style="superscript">r</hi>
               <lb facs="#facs_215_r2l13" n="N014"/><choice><expan>Summa</expan><abbr>Sa</abbr></choice> 33345 fl 32 x<hi style="superscript">r</hi>
               <lb facs="#facs_215_r2l14" n="N015"/>daß also über Abzug deren Creditoren <choice><expan/><abbr>pr</abbr></choice> 9259 fl 3 x<hi style="superscript">r</hi>
               <lb facs="#facs_215_line_1621472011293_4177" n="N016"/>an eigenthum verbleiben 24086 fl 9 x<hi style="superscript">r</hi>
               <lb facs="#facs_215_r2l19" n="N017"/><comment comment="Verbesserung\u0020der\u0020ersten\u00203\u0020Ziffern\u0020der\u002024086\u0020fl">240</comment>
