            <pb facs="#facs_77" n="77" xml:id="img_0077" corresp="https://id.acdh.oeaw.ac.at/wkfm/Merkantil_und_Wechselgericht_Merkantilprotokoll_1_Reihe__Reihe_1_7_Protokoll_1_1429.jpg"/>
            
            <ab facs="#facs_77_r1">
               <lb facs="#facs_77_r1l2" n="N001"/>707
               <lb facs="#facs_77_r1l1" n="N002"/><seg type="orighead__name">Nahmen und Class</seg>
               <lb facs="#facs_77_r1l3" n="N003"/><rs type="person" ref="#wkfm__46169">Rathgeb Melchior</rs> burgerlicher Handlsmann
               <lb facs="#facs_77_r1l4" n="N004"/><seg type="orighead__firma">Firma ad Raggion</seg>
               <lb facs="#facs_77_r1l5" n="N005"/><hi>Melchior Rathgeb laut</hi>
               <lb facs="#facs_77_r1l6" n="N006"/><rs type="person" ref="#wkfm__46730">Balthasar Johann Rathgeb <choice><expan/><abbr>seel:</abbr></choice> Söhne</rs> laut Anbringen de ps: 4<hi style="superscript">ten</hi> <choice><expan/><abbr>Apr:</abbr></choice> 740 N<hi style="superscript">o</hi> 647
               <lb facs="#facs_77_r1l7" n="N007"/><rs type="person" ref="#wkfm__46743">Melchior Rathgeb</rs> laut oblatorien sine dato N<hi style="superscript">o</hi> 1135 und <hi>ab</hi> Anbringen de ps: 3<hi style="superscript">ten</hi> <choice><expan/><abbr>Mart:</abbr></choice>
               <lb facs="#facs_77_r1l8" n="N008"/>746 N<hi style="superscript">o</hi> 2035
               <lb facs="#facs_77_r1l9" n="N009"/><seg type="orighead__owner">Procura und Firmae Trager</seg>
               <lb facs="#facs_77_r1l10" n="N010"/><seg type="orighead__oblatorien">Oblatorien und Avocatorien</seg>
               <lb facs="#facs_77_r1l11" n="N011"/>Mitls Anbringen de ps: 3<hi style="superscript">ten</hi> <choice><expan/><abbr>Mart:</abbr></choice> 746 N<hi style="superscript">o</hi> 2035 hat der <hi>Rathgeb</hi> <rs type="person" ref="#wkfm__46169">Melchior Rathgeb</rs> die
               <lb facs="#facs_77_r1l12" n="N012"/>von ihm dahin erlassene oblatorien,daß <hi>die neg</hi> sein bruder ihm das negotium allein
               <lb facs="#facs_77_r1l13" n="N013"/>überlassen und er solches <hi>unter seiner eignen</hi> wie es unter der raggion <rs type="person" ref="#wkfm__46702">Bathasar Johann
               <lb break="no" facs="#facs_77_r1l14" n="N014"/>Rathgeb</rs> und <rs type="person" ref="#wkfm__46730">Balthasar Johann Rathgeb <choice><expan/><abbr>seel:</abbr></choice> Söhne</rs> geführet worden <hi>word</hi> allein 
               <lb facs="#facs_77_r1l15" n="N015"/>unter der raggion <rs type="person" ref="#wkfm__46743">Melchior Rathgeb</rs> und alleinig eigenhändiger firma, wie solche
               <lb facs="#facs_77_r1l16" n="N016"/>beygeruket continuiren werden sine dato N<hi style="superscript">o</hi> 1135 in <choice><expan/><abbr>orig:</abbr></choice> <hi>eingel</hi> eingeleget
               <lb facs="#facs_77_r1l17" n="N017"/><seg type="orighead__fundi">Fundi Ausweisung</seg>
            </ab>
            
        </div>
      </body>
