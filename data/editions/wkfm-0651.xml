            <pb facs="#facs_31" n="31" xml:id="img_0031" corresp="https://id.acdh.oeaw.ac.at/wkfm/Merkantil_und_Wechselgericht_Merkantilprotokoll_1_Reihe__Reihe_1_7_Protokoll_1_0651.jpg"/>
            
            <ab facs="#facs_31_r1">
               <lb facs="#facs_31_r1l2" n="N001"/>323
               <lb facs="#facs_31_r1l1" n="N002"/><seg type="orighead__name">Nahmen und Class</seg>
               <lb facs="#facs_31_r1l3" n="N003"/><rs type="person" ref="#wkfm__46423">Eberl Johann Caspar</rs> burgerlicher Handlsmann
               <lb facs="#facs_31_r1l4" n="N004"/><seg type="orighead__firma">Firma ad Raggion</seg>
               <lb facs="#facs_31_r1l5" n="N005"/><rs type="person" ref="#wkfm__46424">Johann <hi>Caspar</hi> Casper Eberl</rs> laut Erklährung sine dat N<hi style="superscript">o</hi> 311 welche
               <lb facs="#facs_31_r1l6" n="N006"/>die Vorstehere des burgerlichen Handlsstand mitls Anbringen de ps. 8<hi style="superscript">ten</hi> <choice><expan/><abbr>9br</abbr></choice> 736
               <lb facs="#facs_31_r1l7" n="N007"/>N<hi style="superscript">o</hi> 312 eingeleget
               <lb facs="#facs_31_r1l8" n="N008"/><seg type="orighead__owner">Procura und Firmae Trager</seg>
               <lb facs="#facs_31_r1l9" n="N009"/><seg type="orighead__oblatorien">Oblatorien und Avocatorien</seg>
               <lb facs="#facs_31_r1l10" n="N010"/><seg type="orighead__fundi">Fundi Ausweisung</seg>
               <lb facs="#facs_31_r1l11" n="N011"/>Der <rs type="person" ref="#wkfm__46423">Eberl</rs> hat zu Ausweisung seines Fundi <hi>mit den</hi> mitls Anbringen de ps:
               <lb facs="#facs_31_line_1619281520756_128" n="N012"/>26<hi style="superscript">ten</hi> <choice><expan/><abbr>Aug:</abbr></choice> 737 N<hi style="superscript">o</hi> 1406 den von seiner Ehewirthin <rs type="person" ref="#wkfm__46425">Juliana Barbara</rs> untern
               <lb facs="#facs_31_line_1619284686260_151" n="N013"/>23<hi style="superscript">ten</hi> <choice><expan/><abbr><seg type="editorial_comment_seg">ej:<note type="editorial_comment_note">ejusdem (desselben)</note></seg></abbr></choice> <hi>ausa</hi> dahir ausgestelten <unclear>Cotionirten</unclear> <hi>Revers</hi> <choice><expan/><abbr>orig.</abbr></choice> Revers N<hi style="superscript">o</hi> 1405, daß
               <lb facs="#facs_31_r1l16" n="N014"/>sie von dem ihrigen zur Sicherheit deren Handlungs Creditoren <unclear>gewidmet</unclear>
               <lb facs="#facs_31_line_1620595856586_266" n="N015"/>haben wolle 2000 fl
            </ab>
            
        </div>
