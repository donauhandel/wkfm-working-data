            <ab facs="#facs_33_r1">
               <lb facs="#facs_33_line_1625040508852_724" n="N001"/>655
               <lb facs="#facs_33_r1l1" n="N002"/><seg type="name">Nahmen und Class</seg>
               <lb facs="#facs_33_r1l2" n="N003"/><rs type="person" ref="wkfm__43998">Nigg Ignaz</rs> burgerlicher Handlsmann
               <lb facs="#facs_33_r1l3" n="N004"/>Firma ad Raggion
               <lb facs="#facs_33_r1l4" n="N005"/><seg type="owner">Procura und Firmae Trager</seg>
               <lb facs="#facs_33_r1l5" n="N006"/><seg type="oblatorien">Oblatorien und Avocatorien</seg>
               <lb facs="#facs_33_r1l6" n="N007"/><seg type="fundi">Fundi Ausweisung</seg>
               <lb facs="#facs_33_r1l7" n="N008"/>Der <rs type="person" ref="wkfm__43998">Nigg</rs> hat seinen Handlungs fundum mitls der Specification N<hi style="superscript">o</hi> 1654
               <lb facs="#facs_33_r1l8" n="N009"/>volgender maßen ausgewisen, nemlich daß er eigenthumlich besize
               <lb facs="#facs_33_r1l9" n="N010"/>an erspahrten salario vermög attestati des<rs type="person" ref="wkfm__37748"> Johann Dietschi</rs> dd<hi style="superscript">to</hi> 17<hi style="superscript">ten</hi> 
               <lb facs="#facs_33_line_1621463946943_1690" n="N011"/><choice><expan/><abbr>7br:</abbr></choice> 752 N<hi style="superscript">o</hi> 1655 2000 fl
               <lb facs="#facs_33_r1l14" n="N012"/>dann an Überkommenen Nuzungs Antheil von <hi>Johann</hi>
               <lb facs="#facs_33_r1l15" n="N013"/><rs type="person" ref="wkfm__44039">Dominico Bormastini</rs> laut dessen attestati dd<hi style="superscript">to</hi> 15<hi style="superscript">ten</hi> <choice><expan/><abbr>7br:</abbr></choice>
               <lb facs="#facs_33_line_1621463956772_1697" n="N014"/>752 N<hi style="superscript">o</hi> 1656 5000 fl
               <lb facs="#facs_33_r1l18" n="N015"/>zusamen Eigenthum 7000 fl
               <lb facs="#facs_33_r1l20" n="N016"/>zu Completirung des ubrigen hat der <rs type="person" ref="wkfm__37748">Johann Dietscho</rs> das
               <lb facs="#facs_33_r1l21" n="N017"/><choice><expan/><abbr>orig:</abbr></choice> Cautions dd<hi style="superscript">to</hi> 27<hi style="superscript">ten</hi> <choice><expan/><abbr>7br:</abbr></choice> 752 N<hi style="superscript">o</hi> 1657 eingeleget <choice><expan/><abbr>pr</abbr></choice> 1000 fl
               <lb facs="#facs_33_r1l22" n="N018"/><choice><expan/><abbr>Sa</abbr></choice> 8000 fl
            </ab>
            
