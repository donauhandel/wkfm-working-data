            <pb facs="#facs_237" n="237" xml:id="img_0237" corresp="https://id.acdh.oeaw.ac.at/wkfm/Merkantil_und_Wechselgericht_Merkantilprotokoll_1_Reihe__Reihe_1_7_Protokoll_1_0291.jpg"/>
            
            <ab facs="#facs_237_r1">
               <lb facs="#facs_237_r1l1" n="N002"/><seg type="orighead__name">Nahmen und Class</seg>
               <lb facs="#facs_237_r1l2" n="N003"/><rs type="person" ref="#wkfm__43977">Passy Georg</rs> k. k. Niderlags Verwanter
               <lb facs="#facs_237_r1l3" n="N004"/>Firma ad Raggion
               <lb facs="#facs_237_r1l4" n="N005"/><rs type="person" ref="#wkfm__47385">Georg Passy</rs> laut Anbringen de ps: 26<hi style="superscript">ten</hi> <choice><expan/><abbr>Jun:</abbr></choice> 747 N<hi style="superscript">o</hi> 144
               <lb facs="#facs_237_r1l5" n="N006"/><seg type="orighead__owner">Procura und Firmae Trager</seg>
               <lb facs="#facs_237_r1l6" n="N007"/><seg type="orighead__oblatorien">Oblatorien und Avocatorien</seg>
               <lb facs="#facs_237_r1l8" n="N008"/><seg type="orighead__fundi">Fundi Ausweisung</seg>
               <lb facs="#facs_237_r1l9" n="N009"/>Der <rs type="person" ref="#wkfm__43977">Georg Passy</rs> hat seinen fundum mitls einer <choice><expan/><abbr>orig:</abbr></choice> nota sine dato 
               <lb facs="#facs_237_r1l7" n="N010"/>N<hi style="superscript">o</hi> 1216
               <lb facs="#facs_237_r1l10" n="N011"/>dergestalten ausgewisen, daß er Eigenthumlich besize, <hi>an b</hi>
               <lb facs="#facs_237_line_1621473271140_4780" n="N012"/>an brüderlichen Erbgutt 6500 fl
               <lb facs="#facs_237_r1l13" n="N013"/>dann an überkommenen Anteil aus der Societaet mit <rs type="person" ref="#wkfm__43802">Joh.
               <lb break="no" facs="#facs_237_line_1621473290357_4796" n="N014"/>Baptist Ruard</rs> 13600
               <lb facs="#facs_237_line_1621473298843_4805" n="N015"/>zusamen 20100 fl
               <lb facs="#facs_237_r1l20" n="N016"/>Welche beede Posten er Vermög beygerukter formulae <hi>jurandi</hi>-
               <lb facs="#facs_237_r1l19" n="N017"/>jurandi
               <lb facs="#facs_237_r1l21" n="N018"/>untern 22<hi style="superscript">ten</hi> <choice><expan/><abbr>Jun:</abbr></choice> 747 mit einem Cörperlichen Eyd bekräfftiget.
               <lb facs="#facs_237_r1l22" n="N019"/>Zu Completirung des Übrigen hat selber ein von <rs type="person" ref="#wkfm__30193">Martin
               <lb break="no" facs="#facs_237_r1l23" n="N020"/>Peter Hillebrandt</rs> ausgesteltes <choice><expan/><abbr>orig:</abbr></choice> <choice><expan>Cautions</expan><abbr>Caut:</abbr></choice> Instrument dd<hi style="superscript">to</hi>
               <lb facs="#facs_237_line_1621473356903_4833" n="N021"/><unclear>3<hi style="superscript"/></unclear><hi style="superscript">ten</hi> <choice><expan/><abbr>Apr:</abbr></choice> 747 N<hi style="superscript">o</hi> 1217 <choice><expan/><abbr>pr</abbr></choice> 10000 fl
               <lb facs="#facs_237_r1l26" n="N022"/>Sa 30100<hi>0</hi>
            </ab>
            
        </div>
